        android:icon="@mipmap/ic_launcher"
        android:label="Go Travel"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".LoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".HomePage" />
        <activity android:name=".ResetPassword" />
        <activity android:name=".DaftarTravel" />
        <activity android:name=".AntarLintasSumatra" />
        <activity android:name=".MentariTravel" />
        <activity android:name=".MandalaTravel" />
        <activity android:name=".MenuMedanPadangALS" />
        <activity android:name=".HalamanAkhirALSMP" />
        <activity android:name=".MenuBAcehMedan" />
        <activity android:name=".HalamanAkhirALSBAM" />
        <activity android:name=".MenuBAcehSigliMentaTra" />
        <activity android:name=".HalamanAkhirMentaTBAS" />
        <activity android:name=".MenuBAcehSubulussalam" />
